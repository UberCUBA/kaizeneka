{
  "build": {
    "builder": "dockerfile",
    "dockerfilePath": "api-dotnet/KaizenekaApi/Dockerfile"
  },
  "deploy": {
    "startCommand": "docker run -p $PORT:10000 kaizeneka-api",
    "healthcheckPath": "/",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}